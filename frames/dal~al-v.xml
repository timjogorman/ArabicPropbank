            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>دلّل * على السّلْعة بصوت عال</parse>
                <comment/>
                <arg f="" g="NONE" n="0">-NONE- * ?</arg>
                <arg f="" g="on the item" n="1">على السّلْعة</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="thing auctioned"/>
    </frameset>
    <frameset cdef="" edef="proving" id="03">
