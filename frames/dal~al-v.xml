                <comment/>
            </mapping>
            <example>
                <parse>دلّل *  على المسألة</parse>
                <comment/>
                <arg f="" g="NONE" n="0">-NOONE- * ?</arg>
                <arg f="" g="on the problem" n="1">على المسألة</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="problem"/>
