<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>dal~al</id>
    <comment/>
    <frameset cdef="" edef="spoiling, pampering" id="01">
        <role argnum="0" argrole="spoiler"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>لقد دلل * طفله بسماحه له بالنوم متأخرا</parse>
                <comment/>
                <arg f="" g="NONE" n="0">-NOONE- * ?</arg>
                <arg f="" g="his baby" n="1">طفله</arg>
                <arg f="" g="by letting him to sleep late" n="2">بسماحه له بالنوم متأخرا</arg>
