<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>{isotaqal~</id>
    <comment/>
    <frameset cdef="" edef="to find/consider little" id="01">
        <role argnum="0" argrole="agent"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>استقل الضيف الطعام المقدم اليه</parse>
                <comment/>
                <arg f="" g="the guest" n="0">الضيف</arg>
                <arg f="" g="food provided to him" n="1">الطعام المقدم اليه</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="considered what"/>
    </frameset>
    <frameset cdef="" edef="to posses alone" id="02">
        <role argnum="0" argrole="entity independent"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>من عادته ان يستقل * برأيه</parse>
                <comment/>
                <arg f="" g="he" n="0">-NONE- * ه</arg>
                <arg f="" g="with his opinion" n="1">برأيه</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="with what"/>
    </frameset>
    <frameset cdef="" edef="to be independent" id="03">
        <role argnum="0" argrole="entity getting independent"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>استقلت البلاد منذ سنوات</parse>
                <comment/>
                <arg f="" g="the country" n="0">البلاد</arg>
                <arg f="tmp" g="years ago" n="m">منذ سنوات</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="from what"/>
    </frameset>
    <frameset cdef="" edef="to board, to embark on, to leave, to megrate" id="04">
        <role argnum="0" argrole="entity riding"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>استقل حسام الطائرة من بغداد الى البصرة</parse>
                <comment/>
                <arg f="" g="Husam" n="0">حسام</arg>
                <arg f="" g="the plane" n="1">الطائرة</arg>
                <arg f="" g="from Baghdad" n="2">من بغداد</arg>
                <arg f="" g="to Basra" n="3">الى البصرة</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="riding what"/>
        <role argnum="2" argrole="source (Usually marked by  من)"/>
        <role argnum="3" argrole="end point (Usually marked by  الى)"/>
    </frameset>
    <frameset cdef="" edef="to get hight" id="05">
        <role argnum="0" argrole="entity getting hight"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>استقل النسر  فوق الغيوم</parse>
                <comment/>
                <arg f="" g="the eagle" n="0">النسر</arg>
                <arg f="" g="above the clouds" n="1">فوق الغيوم</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="above what"/>
    </frameset>
</verb>
