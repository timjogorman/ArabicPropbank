<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>raSad-u</id>
    <frameset cdef="" edef="to monitor" id="01">
        <role argnum="0" argrole="observer"/>
        <role argnum="1" argrole="thing looked at"/>
        <frame>
            <example>
                <parse>
                    تقرير يرصد * أوضاع السجون في المغرب
                </parse>
                <arg f="" g="-NONE- &amp;lt; تقرير" n="0">* &amp;lt; تقرير</arg>
                <arg f="" g="the situation of jails in Morroco" n="1">أوضاع السجون في المغرب</arg>
            </example>
        </frame>
    </frameset>
    <frameset cdef="" edef="allocate, give to, donate" id="02">
        <role argnum="0" argrole="granter"/>
        <role argnum="1" argrole="thing granted"/>
        <role argnum="2" argrole="benefactive, granted-to"/>
        <frame>
            <example>
                <parse>
                    بنك إنجلترا يرصد * مائة مليار دولار لمساعدة البنوك
                </parse>
                <arg f="" g="-NONE- &amp;lt; Bank of England" n="0">* &amp;lt; بنك إنجلترا</arg>
                <arg f="" g="one hundred million dollars" n="1">مائة مليار دولار</arg>
                <arg f="" g="to help banks" n="2">لمساعدة البنوك</arg>
            </example>
        </frame>
    </frameset>
</verb>
