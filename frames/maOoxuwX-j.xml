<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>maOoxuwX</id>
    <frameset cdef="" edef="taken / obtained / captured" id="01">
        <role argnum="1" argrole="taken / obtained / captured"/>
        <role argnum="2" argrole="from whom? (usually marked by من)"/>
        <frame>
            <example>
                <parse>
                    جوليو سيزار يسخر من كره كأس العالم و يقول : &amp;quot; كأنها مأخوذه من البقاله
                </parse>
                <arg f="" g="it" n="1">ها</arg>
                <arg f="" g="from the grocery store" n="2">من البقاله</arg>
            </example>
        </frame>
    </frameset>
    <frameset cdef="" edef="surprised / fascinated" id="02">
        <role argnum="1" argrole="entity surprised / fascinated"/>
        <role argnum="2" argrole="from what? (usually marked by ب)"/>
        <frame>
            <example>
                <parse>
                    (NP-SBJ
                    (NP
                    (DET+NOUN+CASE_DEF_NOM العالم))
                    (PP
                    (PREP من)
                    (NP
                    (NP
                    (ADJP
                    (ADJ_COMP اقصا)
                    (NP
                    (POSS_PRON_3MS ه))))
                    (PP
                    (PREP الى)
                    (NP
                    (ADJP
                    (ADJ_COMP اقصا)
                    (NP
                    (POSS_PRON_3MS ه))))))))
                    (ADJP-PRD
                    (ADJ+CASE_INDEF_ACC مأخوذاً)
                    (PP
                    (PREP ب)
                    (NP
                    (NP
                    (DET+NOUN+CASE_DEF_GEN الموضوع)
                    (DET+ADJ+CASE_DEF_GEN العراقي))
                </parse>
                <arg f="" g="it" n="1">? &amp;lt; العالم</arg>
                <arg f="" g="the Iraqi issue" n="2">بالموضوع العراقي</arg>
            </example>
        </frame>
        <comment>
            مندهش / مفتون ب
        </comment>
    </frameset>
    <frameset cdef="" edef="derived" id="03">
        <role argnum="1" argrole="thing derived"/>
        <role argnum="2" argrole="derived from what? (usually marked by عن)"/>
        <frame>
            <example>
                <parse>
                    المسلسل مأخوذ عن حوادث حقيقية
                </parse>
                <arg f="" g="series" n="1">المسلسل</arg>
                <arg f="" g="derived from real accidents" n="2">عن حوادث حقيقية</arg>
            </example>
        </frame>
        <comment>
            مقتبس
        </comment>
    </frameset>
</verb>
