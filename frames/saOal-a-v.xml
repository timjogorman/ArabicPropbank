<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>saOal-a</id>
    <frameset edef="to ask a question/matter" id="01">
        <role argnum="0" argrole="asker"/>
        <role argnum="1" argrole="hearer"/>
        <role argnum="2" argrole="question/matter"/>
        <frame>
            <mapping>
                <V/>
                <mapitem src="sbj" trg="arg0"/>
            </mapping>
            <example>
                <parse>
                    (S (VP (ask/inquire/request+he/it M-saOala- M -سَأَلَ-)
                    (NP-SBJ (-NONE- X))
                    (NP-OBJ (it/him M-hu M -هُ))
                    (S (PRT (does/do?/did?/is/are? Mhal M هَل))
                    (NP-SBJ (bin Mbin M بِن)
                    (Laden/Ladin MlAdin M لادِن))
                    (PP-LOC-PRD (in Mfiy M فِي)
                    (NP (Pakistan MbAkisotAn M باكِسْتان))))))))))
                    (PUNC ,)
                </parse>
                <arg g="NP-SBJ (-NONE- *)" n="0">X</arg>
                <arg g="him" n="1">-hu</arg>
                <arg g="Is Bin Laden in Pakistan ?" n="2">hal bin lAdin fiy bAkisotAn</arg>
                <V/>
            </example>
        </frame>
    </frameset>
</verb>
