<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>{inogalaq</id>
    <comment/>
    <frameset cdef="" edef="To be or get closed/shut, to be sealed" id="01">
        <role argnum="1" argrole="entity/thing closed"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>انغلق عليه الباب</parse>
                <comment/>
                <arg f="" g="on him" n="2">عليه</arg>
                <arg f="" g="the door" n="1">الباب</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="2" argrole="beneficiary"/>
    </frameset>
    <frameset cdef="" edef="To isolate oneself, to seclude oneself" id="02">
        <role argnum="0" argrole="entity isoliting"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>انغلق * على نفسه بعد وفاة والده</parse>
                <comment/>
                <arg f="" g="NONE" n="0">-NONE- *?</arg>
                <arg f="" g="on himself" n="1">على نفسه</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="things isolated"/>
    </frameset>
</verb>
