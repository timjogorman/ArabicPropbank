<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>vab~at</id>
    <frameset cdef="" edef="to fix, to establish, make stable , install software" id="01">
        <role argnum="0" argrole="fixer"/>
        <role argnum="1" argrole="thing fixed"/>
        <frame>
            <example>
                <parse>
                    ثبت الولد برنامج الفوتوشوب
                </parse>
                <arg f="" g="the boy" n="0">الولد</arg>
                <arg f="" g="the photoshop software" n="1">برنامج الفوتوشوب</arg>
            </example>
        </frame>
    </frameset>
    <frameset cdef="" edef="maintain at a fixed level (price)" id="02">
        <role argnum="0" argrole="maintainer"/>
        <role argnum="1" argrole="thing maintained"/>
        <role argnum="2" argrole="level maintained"/>
        <frame>
            <example>
                <parse>
                    ثبت التجار أسعار الفضة
                </parse>
                <arg f="" g="the businessmen" n="0">التجار</arg>
                <arg f="" g="silver prices" n="1">أسعار الفضة</arg>
            </example>
        </frame>
    </frameset>
    <frameset cdef="" edef="to fix one's eyes on, stare at" id="03">
        <role argnum="0" argrole="looker"/>
        <role argnum="1" argrole="looked at"/>
        <frame>
            <example>
                <parse>
                    ثبت الولد في وجه الفتاة
                </parse>
                <arg f="" g="the boy" n="0">الولد</arg>
                <arg f="" g="toward the face of the girl" n="1">في وجه الفتاة</arg>
            </example>
        </frame>
    </frameset>
    <frameset cdef="" edef="to gain or secure a foot , become firmly established" id="04">
        <role argnum="0" argrole="entity establishing"/>
        <role argnum="1" argrole="entity established"/>
        <role argnum="2" argrole="position"/>
        <frame>
            <example>
                <parse>
                    ثبت الموظف الجديد أقدامه في الوظيفة الجديدة
                </parse>
                <arg f="" g="the new employee" n="0">الموظف الجديد</arg>
                <arg f="" g="his feet in the new job" n="1">أقدامه في الوظيفة الجديدة</arg>
            </example>
        </frame>
    </frameset>
</verb>
