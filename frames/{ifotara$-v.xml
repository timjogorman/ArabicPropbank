<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>{ifotara$</id>
    <comment/>
    <frameset cdef="" edef="to lay down on something" id="01">
        <role argnum="0" argrole="entity laying down"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>افترش المسكين الارض</parse>
                <comment/>
                <arg f="" g="the poor" n="0">المسكين</arg>
                <arg f="" g="the floor" n="1">الارض</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="on what"/>
    </frameset>
    <frameset cdef="" edef="to walk through" id="02">
        <role argnum="0" argrole="entity walking"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>افترش * الطريق</parse>
                <comment/>
                <arg f="" g="NONE" n="0">-NONE- *?</arg>
                <arg f="" g="the road" n="1">الطريق</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="the path"/>
    </frameset>
    <frameset cdef="" edef="to defeat" id="03">
        <role argnum="0" argrole="winner"/>
        <frame>
            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>تغالب المتصارعان حتى افترش احدهما الاخر</parse>
                <comment/>
                <arg f="" g="one of them" n="0">احدهما</arg>
                <arg f="" g="the other" n="1">الاخر</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="looser"/>
        <role argnum="2" argrole="event"/>
    </frameset>
    <frameset cdef="" edef="to steal" id="04">
        <role argnum="0" argrole="entity stealing"/>
