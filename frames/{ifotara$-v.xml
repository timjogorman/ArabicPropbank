            <mapping>
                <comment/>
            </mapping>
            <example>
                <parse>افترش * المال</parse>
                <comment/>
                <arg f="" g="NONE" n="0">-NONE- *?</arg>
                <arg f="" g="the money" n="1">المال</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
        <role argnum="1" argrole="things stolen"/>
        <role argnum="2" argrole="from whom"/>
    </frameset>
    <frameset cdef="" edef="to have sex" id="05">
        <role argnum="0" argrole="entity having sex"/>
