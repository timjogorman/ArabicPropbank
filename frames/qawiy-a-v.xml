<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>qawiy-a</id>
    <frameset cdef="" edef="become able" id="01">
        <role argnum="1" argrole="empowered entity"/>
        <role argnum="2" argrole="empowered with"/>
        <frame>
            <example>
                <parse>(SQ
                    (CONJ ف)
                    (PRT
                    (INTERROG_PART هل))
                    (VP
                    (IV3FS+IV+IVSUFF_MOOD:I تقوى)
                    (NP-SBJ
                    (DET+NOUN+NSUFF_FEM_SG+CASE_DEF_NOM الحكومة))
                    (PP-CLR
                    (PREP على)
                    (NP
                    (DEM_PRON_MS ذلك))))
                    (PUNC ?))
                </parse>
                <arg f="" g="the goverment" n="1">الحكومة</arg>
                <arg f="" g="on that ?" n="2">على ذلك?</arg>
                <comment/>
            </example>
            <comment/>
        </frame>
        <comment/>
    </frameset>
    <frameset cdef="" edef="being not able to do something" id="02">
        <role argnum="1" argrole="entity not being able"/>
        <role argnum="2" argrole="ability"/>
        <frame>
            <example>
                <parse/>
                <arg f="" g="" n="">...</arg>
                <comment/>
            </example>
            <comment/>
        </frame>
        <comment/>
    </frameset>
    <comment/>
</verb>
