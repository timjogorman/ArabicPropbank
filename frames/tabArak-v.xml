    <frameset cdef="" edef="To ask the blessing of" id="01">
        <role argnum="0" argrole="entity requesting bless"/>
        <role argnum="1" argrole="bless requested"/>
        <frame>
            <example>
                <parse>
                    (S
                    (VP
                    (CV انثري)
                    (NP-SBJ
                    (-NONE- *))
                    (PP
                    (PREP علي)
                    (NP
                    (PRON_1P نا)))
                    (PP
                    (PREP من)
                    (NP
                    (NOUN+CASE_DEF_GEN ورود)
                    (NP
                    (NOUN+NSUFF_FEM_SG+CASE_DEF_GEN محبت)
                    (NP
                    (POSS_PRON_2FS ك)))))
                    (NP-OBJ
                    (NOUN+CASE_INDEF_ACC اغماراً))
                    (PUNC ,)
                    (PP-PRP
                    (PREP ل)
                    (SBAR
                    (SUB_CONJ كي)
                    (S
                    (S
                    (VP
                    (IV1P+IV+IVSUFF_MOOD:S نتبارك)
                    (NP-SBJ
                    (-NONE- *))
                    (PP
                    (PREP من)
                    (NP
