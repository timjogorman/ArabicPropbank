<!DOCTYPE verb SYSTEM "verb.dtd">
<verb>
    <id>murak~iz</id>
    <frameset cdef="" edef="concentrator" id="01">
        <role argnum="0" argrole="concentrator entity"/>
        <role argnum="1" argrole="what or in what?"/>
        <frame>
            <example>
                <parse>كان الطالب مركزا في المذاكرة</parse>
                <arg f="" g="student" n="0">الطالب</arg>
                <arg f="" g="in study" n="1">في المذاكرة</arg>
                <comment/>
            </example>
            <comment/>
        </frame>
        <comment/>
    </frameset>
    <frameset cdef="" edef="focusing" id="02">
        <role argnum="0" argrole="focusing entity"/>
        <role argnum="1" argrole="on what? ( marked by على)"/>
        <frame>
            <example>
                <parse>(S
                    (VP
                    (PV+PVSUFF_SUBJ:1S وجدت)
                    (NP-SBJ
                    (-NONE- *-1))
                    (S
                    (NP-SBJ
                    (DET+NOUN+CASE_DEF_ACC الاضواء))
                    (ADJP-PRD
                    (ADJ+NSUFF_FEM_SG+CASE_INDEF_ACC مركزة))
                    (PP
                    (PREP علي)
                </parse>
                <arg f="" g="lights" n="0">االاضواء</arg>
                <comment/>
                <arg f="" g="on me" n="1">علي</arg>
            </example>
            <comment/>
        </frame>
        <comment/>
    </frameset>
    <comment/>
</verb>
