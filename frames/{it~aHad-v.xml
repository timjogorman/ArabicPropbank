                </parse>
                <comment/>
                <arg f="" g="Abuu Maazin" n="0">"أبو مازن"</arg>
                <arg f="" g="with Hamaas" n="1">مع حماس</arg>
            </example>
        </frame>
        <comment/>
        <role argnum="1" argrole="thing being united to, if separate from Arg0"/>
